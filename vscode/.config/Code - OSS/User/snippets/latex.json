		"description": "Insert a theorem environment"
	},
	// math snippets
	"math-inline": {
		"prefix": "mk",
		"body": "\\($1\\)$0",
		"description": "Insert an inline math environment"
	},
	"math-display": {
		"prefix": "md",
		"body": [
			"\\[",
			"\t$1",
			"\\]",
			"$0"
		],
		"description": "Insert a display math environment"	
	},
	"piecewise-defined function": {
		"prefix": "switch",
		"body": [
			"\\left\\{ \\begin{array}{ll}",
			"\\t$1",
			"\\end{array} \\right.",
			"$0"
		],
		"description": "Insert a piecewise-defined function"
	},
	"text": {
		"prefix": "txt",
		"body": "\\text{ $1 } $0",
		"description": "Insert a text (inside a math environment)"
	},
	// logical operators
	"logical-not": {
		"prefix": "ln",
