{
	"definition": {
		"prefix": "def",
		"body": [
			"\\begin{definition}{$1}",
			"\t$2",
			"\\end{definition}",
			"$0"
		],
		"description": "Insert a definition environment"
	},
	"theorem": {
		"prefix": "theo",
		"body": [
			"\\begin{theorem}{$1}",
			"\t$2",
			"\\end{theorem}",
			"$0"
		],
		"description": "Insert a theorem environment"
	},
	"lemma": {
		"prefix": "lemma",
